{
  "expo": {
    "name": "marioPartyRandom",
    "slug": "marioPartyRandom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/game-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fa4f79"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/AOTFShinGoProRegular.otf"
          ]
        }
      ]
    ]
  }
}
